{
  "name": "customize-bulma",
  "version": "0.9.0",
  "description": "Create a customized version of Bulma CSS.",
  "main": "src/styles.scss",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/styles.scss dist/styles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settipalli/customize-bulma.git"
  },
  "keywords": [
    "bulma",
    "css",
    "custom",
    "customize"
  ],
  "author": "Santhoshkumar Settipalli",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/settipalli/customize-bulma/issues"
  },
  "homepage": "https://github.com/settipalli/customize-bulma#readme",
  "devDependencies": {
    "bulma": "^0.7.4",
    "node-sass": "^4.11.0"
  }
}
